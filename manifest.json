{
  "manifest_version": 3,
  "name": "Redmine Time Tracker",
  "author": "bisvotti@gmail.com",
  "version": "1.0",
  "description": "Отслеживание учтённого времени в Redmine",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
